summamuuttuja<-function(a, data, max_puuttuvat=1){
data<-data.frame(data)
sarake<-subset(data, select=a)
sarake2<-apply(sarake, 1, mean, na.rm=T)
tulos<-as.vector(sarake2)
return(tulos)
}
