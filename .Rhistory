KeskiarvoJaVali<-function(x, p){
je<-mean(x, na.rm=T)
jee<-mean(x, na.rm=T)-(qt(1-p, df=(length(x)-1))*sd(x, na.rm=T)/sqrt(length(x)))
jeee<- mean(x, na.rm=T)+(qt(1-p, df=(length(x)-1))*sd(x, na.rm=T)/sqrt(length(x)))
tulos<-as.vector(c(je, jee, jeee))
return(tulos)
}
